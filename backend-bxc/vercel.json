{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/perfil/([^/]+)",
      "destination": "/api/perfil/[uid]?uid=$1"
    },
    {
      "source": "/api/boletos/([^/]+)/([^/]+)",
      "destination": "/api/boletos/[id]?id=$1&action=$2"
    },
    {
      "source": "/api/boletos/([^/]+)",
      "destination": "/api/boletos/[id]?id=$1"
    }
  ]
}